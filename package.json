{
  "name": "boba-boba-buyer",
  "version": "1.0.0",
  "description": "Automates buying Boba Boba stock via their Shopify storefront",
  "main": "add-to-cart.js",
  "scripts": {
    "start": "node ./add-to-cart",
    "build": "pkg add-to-cart.js -c package.json --ignore-missing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CampbellPedersen/boba-boba-buyer.git"
  },
  "keywords": [],
  "author": "campbelljip@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CampbellPedersen/boba-boba-buyer/issues"
  },
  "homepage": "https://github.com/CampbellPedersen/boba-boba-buyer#readme",
  "dependencies": {
    "csv": "6.3.10",
    "dotenv": "16.5.0",
    "node": "20.18.0",
    "puppeteer": "24.9.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "pkg": {
    "assets": [
      "node_modules/puppeteer/.local-chromium/**/*"
    ],
    "outputPath": "dist",
    "scripts": [
      "node_modules/csv-parse/sync.js",
      "!node_modules/**/*.d.ts"
    ],
    "targets": [
      "node16-win-x64",
      "node16-macos-x64"
    ]
  }
}
